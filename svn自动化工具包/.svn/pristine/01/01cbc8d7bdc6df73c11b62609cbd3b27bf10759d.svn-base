function addServicePackages()
{
    mainApp.addServicePackage( "JavaProtocolParser", "JavaProtocolParser" );
    mainApp.addServicePackage( "UsgServerMgr", "UsgServerMgr" );
    mainApp.addServicePackage( "DwaServerMgr", "DwaServerMgr" );
    mainApp.addServicePackage( "ServerMgr", "ServerMgr" );
}

function addServiceInstances()
{
    mainApp.addServiceInstance( "JavaProtocolParser", "JavaProtocolParser" );
    mainApp.addServiceInstance( "UsgServerMgr", "UsgServerMgr" );
    mainApp.addServiceInstance( "DwaServerMgr", "DwaServerMgr" );
    mainApp.addServiceInstance( "ServerMgr", "ServerMgr" );
}
