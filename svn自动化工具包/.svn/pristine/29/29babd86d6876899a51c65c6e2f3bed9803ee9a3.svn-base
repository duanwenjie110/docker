function addServicePackages()
{
    mainApp.addServicePackage( "ProtocolGbt", "ProtocolGbt" );
    mainApp.addServicePackage( "LinkMgr", "LinkMgr" );
    mainApp.addServicePackage( "UsgServerMgr", "UsgServerMgr" );
}

function addServiceInstances()
{
    mainApp.addServiceInstance( "ProtocolGbt", "ProtocolGbt" );
    mainApp.addServiceInstance( "LinkMgr", "LinkMgr" );
    mainApp.addServiceInstance( "UsgServerMgr", "UsgServerMgr" );
}
